<!DOCTYPE html>
<html lang="en">
  <%- include("./components/head.ejs") %>
  <body>
    <%- include("./components/navbar.ejs") %>
    <div class="container">
      <% if(blogs.length){ %> <% blogs.map(blog => {%>
      <div class="card">
        <h1><%= blog.title %></h1>
        <p><%= blog.intro %></p>
      </div>
      <% }) %> <% } else {%>
      <h2>No Blogs found</h2>
      <% } %>
    </div>
  </body>
</html>
